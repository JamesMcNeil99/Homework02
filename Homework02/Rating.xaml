<Window x:Class="Homework02.Rating"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework02"
        mc:Ignorable="d"
        Title="Rating" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cmbRating" HorizontalAlignment="Center" Margin="0,137,0,0" VerticalAlignment="Top" Width="259" Height="51" IsEditable="True" FontSize="24"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="101,313,0,0" VerticalAlignment="Top" Height="57" Width="114" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="577,313,0,0" VerticalAlignment="Top" Height="57" Width="114" Click="Cancel"/>

    </Grid>
</Window>
