<Window x:Class="Homework02.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework02"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="SFA_Logo" HorizontalAlignment="Center" Height="178" Margin="0,42,0,0" VerticalAlignment="Top" Width="178" Source="https://www.sfasu.edu/themes/custom/sfa/css/images/logo.png"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,256,0,0" Text="Welcome to the SFASU Theatre" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="40" FontWeight="Bold"/>
        <Button x:Name="btnOK" Content="Ok" HorizontalAlignment="Center" Margin="0,353,0,0" VerticalAlignment="Top" Height="54" Width="128" Click="Open_Next"/>

    </Grid>
</Window>
